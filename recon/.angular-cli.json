{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "recon"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap-theme.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "assets/css/bootstrap-toggle.min.css",
        "assets/css/dataTables.bootstrap.min.css",
        "assets/css/main.css",
        "assets/css/demo.css",
        "styles.css",
        "../node_modules/jquery-confirm/dist/jquery-confirm.min.css",
        "../node_modules/ladda/dist/ladda.min.css",
        "../node_modules/ladda/dist/ladda-themeless.min.css",
        "../node_modules/datatables.net-colreorder-dt/css/colReorder.dataTables.css",
        "../node_modules/datatables.net-buttons-dt/css/buttons.dataTables.css",
        "../src/assets/fonts/Bell MT.css",
        "../node_modules/print-js/dist/print.min.css",
        "../node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
        "../node_modules/ng2-tree/styles.css",
        "../node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
        "../node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
        "../node_modules/jquery-datatables-checkboxes/css/dataTables.checkboxes.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/datatables.net/js/jquery.dataTables.js",
        "../node_modules/jquery-confirm/dist/jquery-confirm.min.js",
        "../node_modules/ladda/dist/ladda.min.js",
        "../node_modules/ladda/dist/spin.min.js",
        "assets/js/dataTables.bootstrap.min.js",
        "../node_modules/datatables.net-colreorder/js/dataTables.colReorder.js",
        "../node_modules/datatables.net-buttons/js/dataTables.buttons.js",
        "../node_modules/datatables.net-buttons/js/buttons.colVis.js",
        "../node_modules/datatables.net-buttons/js/buttons.flash.js",
        "../node_modules/datatables.net-buttons/js/buttons.html5.js",
        "../node_modules/datatables.net-buttons/js/buttons.print.js",
        "../node_modules/jquery-datatables-checkboxes/js/dataTables.checkboxes.min.js",
        "assets/js/responsive.bootstrap.min.js",
        "assets/js/dataTables.responsive.min.js",
        "assets/js/bootstrap-toggle.min.js",
        "assets/js/dataTables.bootstrap.min.js",
        "../node_modules/pdfmake/build/pdfmake.js",
        "../node_modules/pdfmake/build/vfs_fonts.js",
        "../node_modules/print-js/dist/print.min.js",
        "../node_modules/rete/build/rete.min.js",
        "../node_modules/rete-connection-plugin/build/connection-plugin.min.js",
        "../node_modules/rete-vue-render-plugin/build/vue-render-plugin.min.js",
        "../node_modules/vue/dist/vue.js",
        "../node_modules/rete-alight-render-plugin/build/alight-render-plugin.min.js",
        "../node_modules/rete-context-menu-plugin/build/context-menu-plugin.min.js",
		"../node_modules/chart.js/dist/Chart.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "staging": "environments/environment.staging.ts",
        "prod-slice": "environments/environment.prod-slice.ts",
        "prod-uni": "environments/environment.prod-uni.ts",
        "prod-sahaj": "environments/environment.prod-sahaj.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
